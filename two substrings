public class Solution {

    public static void twoSubstrings(String s) {
        // Write your code here
        String a=s,s1="";
        int n= s.length();
        for(int i =n-1;i>=0;i--)
        {
            s1=s1+s.charAt(i);
        }
        if(a.equals(s1))
        {
            System.out.println("NO");
        }
        else
        {
            System.out.println("YES");
        }
    }
}
