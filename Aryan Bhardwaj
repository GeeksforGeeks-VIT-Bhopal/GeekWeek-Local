//TIC TAC TOE
class Node:
	def __init__(self, data):
		self.data = data
		self.next = None

class LinkedList:
	def __init__(self):
		self.head = None
	def printList(self):
		temp = self.head
		while temp:
			print(temp.data, end=" ")
			temp = temp.next
	def addToList(self, newData):
		newNode = Node(newData)
		if self.head is None:
			self.head = newNode
			return

		last = self.head
		while last.next:
			last = last.next

		last.next = newNode
def merger(headA, headB):
    
	trialn = Node(0)

	tail = trialn
	while True:

		if headA is None:
			tail.next = headB
			break
		if headB is None:
			tail.next = headA
			break
		if headA.data <= headB.data:
			tail.next = headA
			headA = headA.next
		else:
			tail.next = headB
			headB = headB.next
		tail = tail.next
	return trialn.next
LIST1 = LinkedList()
LIST2 = LinkedList()
elelink1=int(input("Please enter number of element in linked list1:- "))
for i in range(elelink1):
    ele=int(input("Please enter elemetn no"+str(i)+":- "))
    LIST1.addToList(ele)
    
elelink1=int(input("Please enter number of element in linked list2:- "))
for i in range(elelink1):
    ele=int(input("Please enter elemetn no"+str(i)+":- "))
    LIST2.addToList(ele)
    
LIST1.head = merger(LIST1.head, LIST2.head)

print("Merged Linked List is:")
LIST1.printList()
