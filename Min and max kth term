import array as ar
a=ar.array('i',[1,8,3,9,4,5,6])
b=ar.array(a.typecode,[ele for ele in sorted(a)])
c=int(input("Enter kth value for maximum number: "))
d=int(input("Enter kth value for minimum number: "))
print("Maximum value for given kth term in array is: ",b[-c]) 
print("Minimum value for given kth term in array is: ",b[d-1])
