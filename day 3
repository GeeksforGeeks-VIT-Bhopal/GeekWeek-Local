#include<iostream>
using namespace std;
int solveMeFirst(int a,int b){
    int sum;
    sum = a + b;
    return sum;
}
int main(){
    int a;
    int b;
    cin>>a;
    cin>>b;
    int sum = solveMeFirst(a,b);
    cout<<sum<<endl;
    return 0;
}
#include<iostream>
using namespace std;
int simpleArraySum(int arr[],int n){
    int sum  = 0;
    for(int i=0;i<n;i++)
      sum += arr[i];
    return sum;  
    
}
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int sum = simpleArraySum(arr,n);
    cout<<sum<<endl;
    return 0;
}

#include<iostream>
using namespace std;
long long int sum(long long int arr[]){
    long long int s=0;
    for(int i=0;i<5;i++)
      s += arr[i];
    return s;  
}
long long int max(long long int arr[]){
    long long int h= arr[0];
    for(int i=1;i<5;i++){
        if(arr[i]>h)
          h = arr[i];
    }
    return h;
}
long long int min(long long int arr[]){
    long long int m = arr[0];
    for(int i=1;i<5;i++){
        if(arr[i]<m)
          m = arr[i];
    }
    return m;
    
}
int main(){
    long long int arr[5];
    for(int i=0;i<5;i++)
      cin>>arr[i];
    long long int m;
    long long int h;
    long long int s;
     m = min(arr);
     h= max(arr);
     s = sum(arr);
     cout<<s-h<<" "<<s-m<<endl;
     return 0;
      
}

